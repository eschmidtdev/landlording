<p id="notice"><%= notice %></p>

<h1>E Forms</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @e_forms.each do |e_form| %>
      <tr>
        <td><%= e_form.name %></td>
        <td><%= e_form.user_id %></td>
        <td><%= link_to 'Show', e_form %></td>
        <td><%= link_to 'Edit', edit_e_form_path(e_form) %></td>
        <td><%= link_to 'Destroy', e_form, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New E Form', new_e_form_path %>
