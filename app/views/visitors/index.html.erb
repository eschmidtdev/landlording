<div class="container">

  <!-- Heading -->
  <div class="col-md-12 my-4 p-0">
    <span class="page_heading">Account Overview</span>
  </div>

  <div class="card">
    <div class="card-body">
      <div class="row">
        <div class="col-md-12">
          <div class="col-md-3">
            <img src="Icon.svg" class="card_image">
          </div>
          <div class="col-md-6">
            <h3><b>My Documents</b></h3>
            <p class="yield_description">Create, sign and manage lease agreements, notice forms and other documents.</p>
          </div>
          <div class="col-md-3">
            <img src="<%= current_page?(visitors_url) ? 'active_arrow.svg' : 'inactive_arrow.svg' %>" style="margin-top: 4.5vh;">
          </div>
        </div>
      </div>
    </div>
  </div>

  <div class="card mt-3">
    <div class="card-body">
      <div class="row">
        <div class="col-md-12">
          <div class="col-md-3">
            <img src="Rental.svg" class="card_image">
          </div>
          <div class="col-md-6">
            <h3><b>Rental Applications</b></h3>
            <p class="yield_description">Easily screen tenants with our online rental application form.</p>
          </div>
          <div class="col-md-3"><img src="inactive_arrow.svg" style="margin-top: 4.5vh;"></div>
        </div>
      </div>
    </div>
  </div>

  <div class="card mt-3">
    <div class="card-body">
      <div class="row">
        <div class="col-md-12">
          <div class="col-md-3">
            <img src="MyProp.svg" class="card_image">
          </div>
          <div class="col-md-6">
            <h3><b>My Properties</b></h3>
            <p class="yield_description">Save and review information about your rental properties and tenants.</p>
          </div>
          <div class="col-md-3"><img src="inactive_arrow.svg" style="margin-top: 4.5vh;"></div>
        </div>
      </div>
    </div>
  </div>

  <div class="card mt-3">
    <div class="card-body">
      <div class="row">
        <div class="col-md-12">
          <div class="col-md-3">
            <img src="AccountSet.svg" class="card_image">
          </div>
          <div class="col-md-6">
            <h3><b>Account Settings</b></h3>
            <p class="yield_description">view and edit login, contact info and subscription details.</p>
          </div>
          <div class="col-md-3"><img src="inactive_arrow.svg" style="margin-top: 4.5vh;"></div>
        </div>
      </div>
    </div>
  </div>

</div>